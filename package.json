{
  "name": "vitepress-plugin-diagrams",
  "version": "1.1.1",
  "description": "VitePress plugin for generating diagrams from text (PlantUML, Mermaid, etc.)",
  "author": "Ruslan Makarov <ruslan.makarov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/altrusl/vitepress-plugin-diagrams.git"
  },
  "bugs": {
    "url": "https://github.com/altrusl/vitepress-plugin-diagrams/issues"
  },
  "homepage": "https://github.com/altrusl/vitepress-plugin-diagrams",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "vite",
    "vitepress-plugin",
    "diagrams",
    "plantuml",
    "mermaid"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "bin": "bin/cli.js",
  "scripts": {
    "build": "vite build && pnpm run build:types",
    "build:types": "tsc -p tsconfig.types.json",
    "build:cli": "tsc",
    "publish": "pnpm publish --access public"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^4.6.0",
    "@types/node": "^22.13.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.20.0",
    "typescript": "^5.7.3",
    "undici-types": "^7.10.0",
    "vite": "^6.1.0",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vite": ">= 4.0.0",
    "vitepress": ">= 1.0.0"
  }
}
